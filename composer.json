{
    "name": "avvertix/kart-race-manager",
    "type": "project",
    "description": "Kart Race Management. Collect and verify racers before entering the race track.",
    "keywords": ["racing", "kart", "pilot-registration", "results", "transponder", "MyLaps"],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "andreiio/blade-remix-icon": "^3.6",
        "barryvdh/laravel-dompdf": "^3.1",
        "blade-ui-kit/blade-icons": "^1.7",
        "laravel/framework": "^12.0",
        "laravel/jetstream": "^5.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.10.1",
        "league/flysystem-path-prefixing": "^3.23",
        "livewire/livewire": "^3.4",
        "maatwebsite/excel": "^3.1",
        "rinvex/countries": "^9.0",
        "saloonphp/xml-wrangler": "^1.3",
        "spatie/icalendar-generator": "*",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-data": "^4.5",
        "timokoerber/laravel-one-time-operations": "^1.4",
        "vitorccs/laravel-csv": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.12",
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^11.5.3",
        "plannr/laravel-fast-refresh-database": "^1.2",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php artisan config:clear",
            "@php artisan clear-compiled",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/pint"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
